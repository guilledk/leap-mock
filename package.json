{
  "name": "leap-mock",
  "repository": "git+https://github.com/guilledk/leap-mock.git",
  "version": "0.1.1-rc6",
  "description": "AntelopeIO/leap mocker for advanced indexer testing",
  "main": "./build/main.js",
  "type": "module",
  "exports": {
    "./chain.js": "./build/chain.js",
    "./controller.js": "./build/controller.js",
    "./controllerHTTPClient.js": "./build/controllerHTTPClient.js",
    "./controllerRoutes.js": "./build/controllerRoutes.js",
    "./controllerUtils.js": "./build/controllerUtils.js",
    "./httpApiSocket.js": "./build/httpApiSocket.js",
    "./shipSocket.js": "./build/shipSocket.js",
    "./logging.js": "./build/logging.js",
    "./utils.js": "./build/utils.js",

    "./tests/utils.js": "./build/tests/utils.js"
  },
  "scripts": {
    "bootstrap": "sh -c 'yarn && cd node_modules/@eosrio/hyperion-sequential-reader && npm i && npx tsc'",
    "build": "npm run bootstrap && tsc && node scripts/create-dist-packages.mjs",
    "start": "node build/src/main.js",
    "test": "mocha build/tests/**/*.spec.js --timeout 120000",
    "docker-build": "npm run build && node scripts/docker-build.mjs",
    "docker-start": "node scripts/docker-run.mjs",
    "docker-stop": "node scripts/docker-stop.mjs",
    "docker-push": "node scripts/docker-push.mjs"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@greymass/eosio": "0.6.11",
    "@types/express": "^4.17.20",
    "@types/ws": "^8.5.8",
    "async": "^3.2.4",
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@eosrio/hyperion-sequential-reader": "https://github.com/eosrio/hyperion-sequential-reader.git#hist_buf_size",
    "@types/async": "^3.2.16",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2"
  }
}
